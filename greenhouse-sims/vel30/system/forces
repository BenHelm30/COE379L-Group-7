forces
{
    type          forces;

    libs          ("libforces.so");

    writeControl  timeStep;
    timeInterval  1;

    log           yes;

    // specify the patches on which to compute forces
    patches       (greenhouse);

    // moments
    CofR          (0 0 0);    // Centre of rotation for moment calculations 
    pitchAxis     (0 1 0);    // Pitch axis, y

    // do not change
    rho           rhoInf;     // Incompressible
    rhoInf        1.0;          // Incompressible

}


