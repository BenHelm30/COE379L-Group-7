FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "include/inputs"

Ux #calc "cos($rotAngle*3.14159265359/180.0)";
Uy #calc "sin($rotAngle*3.14159265359/180.0)";

dimensions      [0 1 -1 0 0 0 0];

internalField  uniform (0.0 0.0 0.0);

boundaryField
{

    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    outlet
    {
        type            zeroGradient; 
    }

    inlet 
    {
        type            fixedValue; 
        value           uniform ($Ux $Uy 0);
    }

    bottomWall
    {
        type            slip;
    }

    topWall
    {
        type            slip;
    }

    greenhouse
    {
        type            noSlip;
    }

    yMinusWall 
    {
        type            slip;
    }

    yPlusWall 
    {
        type            slip;
    }

}


