FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "include/inputs"

// see discussion at https://turbmodels.larc.nasa.gov/spalart.html
nutInf #calc "0.210438*$nu";

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform $nutInf;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    outlet
    {
        type            zeroGradient; 
    }

    inlet 
    {
        type            fixedValue;
        value           uniform $nutInf;
    }

    bottomWall
    {
        type            zeroGradient; 
    }

    topWall
    {
        type            zeroGradient; 
    }

    wing
    {
        type            fixedValue;
        value           uniform 0.0; 
    }

    yMinusWall 
    {
        type            zeroGradient; 
    }

    yPlusWall 
    {
        type            zeroGradient; 
    }
}


